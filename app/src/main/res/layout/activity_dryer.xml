                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_assets_lightbluebell" />

                <TextView
                    android:id="@+id/Dryer9th_timevalue"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/dryer9th_time_value"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@+id/Dryer9th_name"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/Dryer9th_timestatus"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="8dp"
                    android:text="@string/dryer9th_time_status"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/Dryer9th_name" />
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
    </android.support.v4.widget.SwipeRefreshLayout>
    <android.support.design.widget.BottomNavigationView
        android:id="@+id/navigation"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
